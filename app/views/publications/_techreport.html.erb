<% fields_for techreport do |f| %>
  <p>
    <%= f.label :abstract %><br />
    <%= f.text_area :abstract %>
  </p>
  <p>
    <%= f.label :publication_date %><br />
    <%= f.date_select :publication_date %>
  </p>
  <p>
    <%= f.label :publisher %><br />
    <%= f.text_field :publisher %>
  </p>
  <p>
    <%= f.label :edition %><br />
    <%= f.text_field :edition %>
  </p>
  <p>
    <%= f.label :series %><br />
    <%= f.text_field :series %>
  </p>
  <p>
    <%= f.label :number %>
    <%= f.text_field :number, :size => 4 %>
  </p>
<% end %>
